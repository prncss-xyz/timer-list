{
  "name": "timer-list",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage=true --changedSince=origin/main",
    "test:debug": "jest -o --watch --coverage=false",
    "test:updateSnapshots": "jest -u --coverage=false",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export -p web"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(?:.pnpm/)?((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg))"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/roboto-mono": "^0.2.3",
    "@expo/metro-runtime": "~3.1.3",
    "@react-native-async-storage/async-storage": "1.21",
    "bunshi": "^2.1.4",
    "expo": "~50.0.14",
    "expo-av": "^13.10.5",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.11",
    "expo-font": "^11.10.3",
    "expo-keep-awake": "^12.8.2",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.8",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.4",
    "jotai": "^2.7.1",
    "jotai-effect": "^0.6.0",
    "jotai-optics": "^0.3.2",
    "optics-ts": "^2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-uuid": "^2.0.2",
    "react-native-web": "~0.19.10",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.67",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "fast-check": "^3.16.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "private": true
}
